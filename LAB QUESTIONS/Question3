#include <iostream>
int main() {
    const int arraySize = 7;
    std:: string myArray[arraySize] = {"B123", "C234", "C15", "B177", "G3003", "C235", B179""};
    for(int i =0; i<arraySize; i++){
        if (myArray[i].substr(0 , 1)=="B") {
            std:: cout << myArray[i] << std::endl;
        }
    }
    return0;
}